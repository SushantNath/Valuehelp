<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Product2"
		ok=".onValueHelpOkPress2"
		cancel=".onValueHelpCancelPress2"
		afterClose=".onValueHelpAfterClose2"
		supportRanges="true"
		key="ProductId"
		descriptionKey="Name"
			supportMultiselect="true"
		
	>
		<filterBar>
			<fb:FilterBar advancedMode="false" search=".onFilterBarSearch2">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Ship To"
						label="Ship to"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="Fkart" id="shipToInputId" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Text"
						label="Text"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="Vtext"  id="textInputId"/>
						</fb:control>
					</fb:FilterGroupItem>

				
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
		
		
	</ValueHelpDialog>
</core:FragmentDefinition>
